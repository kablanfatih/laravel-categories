{
    "name": "artisanry/categories",
    "description": "Associate Eloquent Models with Categories",
    "keywords": [
        "laravel",
        "framework",
        "categories",
        "polymorphic",
        "eloquent"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "~5.8|^6.5.0",
        "illuminate/support": "~5.8|^6.5.0",
        "spatie/laravel-sluggable": "^2.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Categories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Categories\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Categories\\CategoriesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
